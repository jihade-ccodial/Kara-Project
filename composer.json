{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "google/apiclient": "^2.15",
        "guzzlehttp/guzzle": "^7.8",
        "hubspot/api-client": "^9.0",
        "laravel/fortify": "^1.20",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.15",
        "laravel/tinker": "^2.10",
        "laravolt/avatar": "^6.0",
        "livewire/livewire": "^3.5",
        "pricecurrent/laravel-eloquent-filters": "^0.1.2",
        "socialiteproviders/hubspot": "^4.1",
        "spatie/laravel-backup": "^9.0",
        "spatie/laravel-collection-macros": "^8.0",
        "spatie/laravel-flash": "^1.9",
        "torann/currency": "^1.1",
        "yajra/laravel-datatables-oracle": "^12.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.23",
        "kkomelin/laravel-translatable-string-exporter": "^1.17",
        "laravel/pint": "^1.14",
        "laravel/sail": "^1.27",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "orangehill/iseed": "^3.0",
        "phpunit/phpunit": "^11.1",
        "spatie/laravel-ignition": "^2.4",
        "spatie/laravel-ray": "^1.38"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helpers/Functions.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
